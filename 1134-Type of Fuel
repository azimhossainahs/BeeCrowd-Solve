#include <stdio.h>
int main(){
    int a=0,g=0,d=0,e=0;
    while(1){
        scanf("%d",&e);
        if(e == 4)
            break;
        else if(e==1)
            a++;
        else if(e == 2)
            g++;
        else if(e == 3)
            d++;
    }
    printf("MUITO OBRIGADO\n");
    printf("Alcool: %d\n",a);
    printf("Gasolina: %d\n",g);
    printf("Diesel: %d\n",d);
    return 0;
}
